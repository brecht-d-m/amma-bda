imports:
- path: container_manifest.yaml

resources:
- name: datalab-vm
  type: compute.v1.instance
  properties:
    zone: us-central1-b
    machineType: https://www.googleapis.com/compute/v1/projects/propane-bearing-124123/zones/us-central1-b/machineTypes/n1-standard-1
    metadata:
      items:
      - google-container-manifest: container_manifest.yaml
    disks:
    - deviceName: boot
      type: PERSISTENT
      boot: true
      autoDelete: true
      sizeGb: 100
      initializeParams:
        diskName: disk-created-by-cloud-config
        sourceImage: https://www.googleapis.com/compute/v1/projects/debian-cloud/global/images/debian-7-wheezy-v20150526
    networkInterfaces:
    - network: $(ref.datalab-network.selfLink)
- name: datalab-network
  type: compute.v1.network
  properties:
    IPv4Range: 10.240.0.0/24

